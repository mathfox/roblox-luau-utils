{
   "name": "roblox-luau-utils",
   "globIgnorePaths": [
      "**/*.spec.lua"
   ],
   "tree": {
      "$className": "DataModel",
      "ReplicatedStorage": {
         "$className": "ReplicatedStorage",
         "$path": "src/shared"
      },
      "ServerStorage": {
         "$className": "ServerStorage",
         "$path": "src/server"
      },
      "StarterPlayer": {
         "$className": "StarterPlayer",
         "StarterPlayerScripts": {
            "$path": "src/client"
         }
      }
   }
}